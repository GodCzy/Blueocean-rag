# Copy this file to `.env` and adjust the values for your deployment.
# Only environment variables declared here are read by ``src.config.settings``.

# Enable debug mode for local development
DEBUG=false

# Optional API key used by protected administrative endpoints
API_KEY=

# Model configuration overrides (leave empty to use defaults)
LLM_MODEL_NAME=
EMBEDDING_MODEL_NAME=
TENSOR_PARALLEL_SIZE=1

# Local cache directories
MODEL_DIR=./models

